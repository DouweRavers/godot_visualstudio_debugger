windows.editor.x86_64 = "res://addons/godot_visualstudio_debugger/cpp/bin/libvsdebugger.windows.editor.x86_64.dll"
linux.editor.x86_64 = "res://addons/godot_visualstudio_debugger/cpp/bin/libvsdebugger.linux.editor.x86_64.so"
linux.editor.arm64 = "res://addons/godot_visualstudio_debugger/cpp/bin/libvsdebugger.linux.editor.arm64.so"
linux.editor.rv64 = "res://addons/godot_visualstudio_debugger/cpp/bin/libvsdebugger.linux.editor.rv64.so"
android.editor.x86_64 = "res://addons/godot_visualstudio_debugger/cpp/bin/libvsdebugger.android.editor.x86_64.so"
